{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "dev:storybook": "storybook dev -p 6006",
    "build:next": "next build",
    "build:storybook": "storybook build -o ./out/storybook -c .storybook",
    "postbuild:storybook": "perl -i -p -e 's#<head>#<head>\n\t\t<base href='/storybook/'>#' ./out/storybook/index.html",
    "start": "serve ./out",
    "lint": "next lint",
    "test:unit": "jest --passWithNoTests --testMatch=**/*.test.ts",
    "test:dev": "cypress open & next dev",
    "test:component": "cypress run --component",
    "test:e2e": "start-server-and-test 3000 'cypress run --e2e'",
    "scripts:generate-imports-script": "tsx scripts/generate-imports-script.ts",
    "scripts:pull-content": "tsx scripts/pull-content.ts",
    "scripts": "run-p scripts:*",
    "storyblok:init": "storyblok pull-components --space=280868",
    "storyblok:generate-types": "storyblok-generate-ts source=./components.280868.json target=./types/storyblok",
    "storyblok": "run-s storyblok:*"
  },
  "dependencies": {
    "@storyblok/js": "^3.0.7",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/nextjs": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "axios": "^1.6.7",
    "classnames": "^2.5.1",
    "dotenv": "^16.4.5",
    "next": "^13.5.6",
    "next-videos": "^1.4.1",
    "normalize.css": "^8.0.1",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.71.1",
    "storybook": "^7.6.17",
    "ua-parser-js": "^1.0.37"
  },
  "devDependencies": {
    "@cypress/skip-test": "^2.6.1",
    "@jest/globals": "^29.7.0",
    "@testing-library/cypress": "^10.0.1",
    "@types/node": "^20.11.27",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/ua-parser-js": "^0.7.39",
    "cypress": "^13.6.6",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "serve": "^14.2.1",
    "start-server-and-test": "^2.0.3",
    "storyblok": "^3.29.1",
    "storyblok-generate-ts": "^2.0.2",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5"
  }
}
